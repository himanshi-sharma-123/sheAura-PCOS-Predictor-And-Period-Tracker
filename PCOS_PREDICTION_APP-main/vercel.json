{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1",
      "dest": "/index.js"
    },
    {
      "src": "/predict-pcos",
      "dest": "/main.py"
    }
  ]
}
